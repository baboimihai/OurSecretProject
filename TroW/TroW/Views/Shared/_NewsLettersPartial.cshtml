@using TroW.Models
@model List<NewsLetterViewModel>
@{
    foreach (var message in Model)
    {
        <div class="list-group">
            <a href="#" class="list-group-item active">
                <h4 class="list-group-item-heading">@message.Header</h4>
                <p class="list-group-item-text">
                    @message.Message
                </p>
            </a>
        </div>
    }
}



